  "version": "0.7.1",
  "description": "Cookie-based sessions for Hono web framework",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaelwd/hono_sessions.git"
  },
  "bugs": {
    "url": "https://github.com/kaelwd/hono_sessions/issues"
